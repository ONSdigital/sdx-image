                  {
                    "id": "answer575c4a60-f021-4a3f-9da9-ccedfd312c2c",
                    "mandatory": true,
                    "type": "TextArea",
                    "label": "Changes to product details",
                    "max_length": 2000,
                    "guidance": {
                      "show_guidance": "What do we mean by ’the price your business charged for this new or changed product’",
                      "hide_guidance": "What do we mean by ’the price your business charged for this new or changed product’",
                      "contents": [
                        {
                          "description": "<strong>The price quoted should:</strong>"
                        },
                        {
                          "list": [
                            "be a <strong>normal export transaction price,</strong> for example, after discounts and excluding VAT and agent’s commission",
                            "be to a specific customer or an agent",
                            "reflects orders placed in that month",
                            "compare <strong>like with like</strong> each month",
                            "be <strong>representative</strong> of current exports."
                          ]
                        },
                        {
                          "description": "If your business has not sold this product in the specified month, enter the previous price."
