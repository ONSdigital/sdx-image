                  {
                    "id": "answerd1cc08eb-0515-4e77-959f-c580da8c25bf",
                    "mandatory": true,
                    "type": "TextArea",
                    "label": "Changes to product details",
                    "max_length": 2000,
                    "guidance": {
                      "show_guidance": "What do we mean by ’the price your business charged for this new or changed product’",
                      "hide_guidance": "What do we mean by ’the price your business charged for this new or changed product’",
                      "contents": [
                        {
                          "description": "<strong>The price quoted should:</strong>"
                        },
                        {
                          "list": [
                            "be a <strong>normal transaction price</strong>, this means after discounts and <strong>excluding</strong> VAT",
                            "be the price you achieve in a significant proportion of UK sales, after discounts and excluding VAT",
                            "reflect orders placed in that month",
                            "compare<strong> like with like each month</strong>",
                            "be <strong>representative </strong>of current output",
                            "normally be sold to customers outside of your company"
                          ]
                        },
                        {
                          "description": "If your business has not sold this product in the specified month, enter the previous price if still representative."
                        },
                        {
                          "description": "Enter the price without any currency symbol. For example, for Pounds Sterling, enter ’1.23’ for ’&#xA3;1.23’. If applicable for the product, provide figures up to 4 decimal places."
                        }
                      ]
                    }
